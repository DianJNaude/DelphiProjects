procedure TfrmGame1.BitBtn1Click(Sender: TObject);
begin
Application.Terminate;
end;

procedure TfrmGame1.bmbHomeClick(Sender: TObject);
begin
frmMain.Show;
frmGame1.Hide  ;
end;

procedure TfrmGame1.btnNextClick(Sender: TObject);
begin
TabSheet3.Show;
TabSheet2.Hide;
end;

procedure TfrmGame1.btnEndClick(Sender: TObject);
begin
 frmMain.Show;
frmGame1.Hide  ;

end;

